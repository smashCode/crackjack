<body>
<a href="http://localhost:3000/" class="btn btn-info">Home</a>
	<table  id="imageTable" class="table table-condensed">
	<thead> 
		<tr><th>Upload ID</th><th>Image Hash</th> <th>Edited on </th>  <th> Edited By</th> <th>previous Node</th></tr>
		</thead>
		<tbody>
			<% updates.forEach(function(update) { %>
					<tr><td><%= update._id %></td><td> <%= update.hash %></td><td><%= update.edited %></td><td>
						<a href="/user/<%= update.editor %>"><%= update.editor %></a></td><td><%= update.previousNode%></td></tr>
			<% }); %>
		</tbody>
</body>