<body>
	<a href="http://localhost:3000/" class="btn btn-info">Home</a>
	<table  id="imageTable" class="table table-condensed">
	<thead> 
		<tr><th>User ID</th><th>User Hash</th> <th>Name </th>  <th> Location</th> <th>Joined On</th></tr>
		</thead>
		<tbody>
			<% users.forEach(function(user) { %>
					<tr><td><%= user._id %></td><td> <%= user.hash %></td><td><%= user.name %></td><td>
						<%= user.location %></td><td><%= user.created_at%></td></tr>
			<% }); %>
		</tbody>
</body>